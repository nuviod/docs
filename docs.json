{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Nuviod",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "General Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "started/welcome",
              "started/dependencies",
              "started/commonerrors",
              "started/faq"
            ]
          },
          {
            "group": "Scripts",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
